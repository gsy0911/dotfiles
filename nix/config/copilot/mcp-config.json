{
  "mcpServers": {
    "serena": {
      "type": "local",
      "command": "uvx",
      "tools": [
        "*"
      ],
      "args": [
        "--from git+https://github.com/oraios/serena serena start-mcp-server --context ide-assistant --project $(pwd)"
      ]
    },
    "fetch-mcp": {
      "type": "local",
      "command": "npx",
      "tools": [
        "*"
      ],
      "args": [
        "mcp-fetch-server"
      ]
    }
  }
}

